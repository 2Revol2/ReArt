{
  "name": "reart",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve  --env port=3000",
    "server": "node ./json-server/index.js",
    "start:dev": "concurrently \"npm start\" \"npm run server\"",
    "build:prod": "webpack --env mode=production apiUrl=https://re-art-backend-bcki.vercel.app",
    "build:dev": "webpack --env mode=development",
    "build:vercel": "webpack",
    "lint": "npx eslint \"**/*.{ts,tsx}\" && npx stylelint \"**/*.scss\"",
    "lint:fix": "npx eslint \"**/*.{ts,tsx}\" --fix && npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
